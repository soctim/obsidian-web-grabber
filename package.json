{
	"name": "obsidian-web-grabber",
	"version": "1.0.0",
	"description": "A plugin for grabbing web content and creating Obsidian notes with configurable parsers and templates",
	"main": "main.js",
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
